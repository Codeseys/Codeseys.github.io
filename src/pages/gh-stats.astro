---
import BaseLayout from '@/layouts/BaseLayout.astro'
import GithubStats from 'astro-github-stats'
---

<BaseLayout title="Heimdall" description="Codeseys's Personal Website">
  <section class="container grid items-center gap-6 pb-8 pt-6 md:py-10">
    <div>
      <!-- show your account stats w/ icons -->
      <GithubStats username="baladithyab" showIcons={true} />

      <!-- show your account top languages -->
      <GithubStats username="baladithyab" topLanguages={true} />
    </div>
  </section>
</BaseLayout>
